	<key>IDECodeSnippetContents</key>
	<string>extension &lt;# View #&gt;SnapshotTests {
    
    func verifyView(with count: Int,
                    position: Int = 0,
                    file: StaticString = #file,
                    line: UInt = #line) {
        
        let viewState = &lt;#View State#&gt;ViewState(&lt;#Params#&gt;)
        let view = &lt;#System Under Test#&gt;(viewState: viewState)
        view.translatesAutoresizingMaskIntoConstraints = false
        view.update(&lt;#Params#&gt;)
        
        view.backgroundColor = .white
        
        FBSnapshotVerifyView(view,
                             file: file,
                             line: line)
    }
    
}
</string>
